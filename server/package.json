{
  "name": "clickup-clone",
  "version": "1.0.0",
  "description": "Clickup clone for this project with NodeJS",
  "author": "Eri Sitohang",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "db:migrate": "knex --knexfile ./src/configs/db.js migrate:latest",
    "db:seed": "knex --knexfile ./src/configs/db.js seed:run",
    "test": "PORT=8001 NODE_ENV=test  mocha --exit \"test/**/*.spec.js\""
  },
  "engines": {
    "node": "14.15.3",
    "npm": "6.14.9"
  },
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "knex": "^0.21.16",
    "pg": "^8.5.1"
  },
  "devDependencies": {
    "faker": "^5.2.0",
    "mocha": "^8.2.1",
    "nodemon": "^2.0.7"
  }
}
